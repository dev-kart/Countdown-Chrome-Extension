{
  "manifest_version": 3,
  "name": "TimeTill",
  "version": "1.3",
  "description": "Countdown to the New Year with customizable styles and themes.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
